    },
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Generated server url"
    }
  ],
  "tags": [
    {
      "name": "Canvas",
      "description": "Controller responsible for calling canvas"
    },
    {
      "name": "Actuator",
      "description": "Monitor and interact",
      "externalDocs": {
        "description": "Spring Boot Actuator Web API Documentation",
        "url": "https://docs.spring.io/spring-boot/docs/current/actuator-api/html/"
      }
    }
  ],
  "paths": {
    "/chat": {
      "post": {
        "tags": [
          "chat"
        ],
        "summary": "Verify Assignment",
        "description": "Generate a prompt to verify your submission",
        "operationId": "verifyAssignment",
        "parameters": [
          {
            "name": "courseId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "assignmentId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "access_token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChatDTO"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "200 Successful Request",
            "content": {
              "text/event-stream": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ChatDTOResponse"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/chat/{lessonId}": {
      "get": {
        "tags": [
          "chat"
        ],
        "summary": "Get Assignment",
        "description": "Get assignment answer by uuid",
        "operationId": "getLessonReviews",
        "parameters": [
          {
            "name": "lessonId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 Successful Request",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/LessonReviews"
                }
              }
            }
          },
          "400": {
            "description": "404 Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/LessonReviews"
                }
              }
            }
          }
        }
      }
    },
    "/canvas/courses": {
      "get": {
        "tags": [
          "Canvas"
        ],
        "summary": "Get all courses enrolled",
        "description": "get all courses which student is enrolled",
        "operationId": "getAllCoursesEnrolled",
        "parameters": [
          {
            "name": "access_token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 Successful Request",
            "content": {
              "*/*": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Course"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/canvas/assignments/{courseId}": {
      "get": {
        "tags": [
          "Canvas"
        ],
        "summary": "Get all Assignment by course",
        "description": "Get all assignments available in the course",
        "operationId": "getAssignmentById",
        "parameters": [
          {
            "name": "courseId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "access_token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 Successful Request",
            "content": {
              "*/*": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Assignment"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/canvas/assignments/{courseId}/{assignmentId}": {
      "get": {
        "tags": [
          "Canvas"
        ],
        "summary": "Get the assignment by id",
        "description": "get assignment by id",
        "operationId": "getAssignmentById_1",
        "parameters": [
          {
            "name": "courseId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "assignmentId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "access_token",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "200 Successful Request",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Assignment"
                }
              }
            }
          }
        }
      }
    },
    "/actuator": {
      "get": {
        "tags": [
          "Actuator"
        ],
        "summary": "Actuator root web endpoint",
        "operationId": "links",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/vnd.spring-boot.actuator.v3+json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "additionalProperties": {
                      "$ref": "#/components/schemas/Link"
                    }
                  }
                }
              },
              "application/vnd.spring-boot.actuator.v2+json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "additionalProperties": {
                      "$ref": "#/components/schemas/Link"
                    }
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "additionalProperties": {
                      "$ref": "#/components/schemas/Link"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/actuator/health": {
      "get": {
        "tags": [
          "Actuator"
        ],
        "summary": "Actuator web endpoint 'health'",
        "operationId": "health",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/vnd.spring-boot.actuator.v3+json": {
                "schema": {
                  "type": "object"
                }
              },
              "application/vnd.spring-boot.actuator.v2+json": {
                "schema": {
                  "type": "object"
                }
              },
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ChatDTO": {
        "type": "object",
        "properties": {
          "subject": {
            "type": "string",
            "description": "The school subject",
            "example": "Database 2"
          },
          "professor": {
            "type": "string",
            "description": "The professor responsible for the subject",
            "example": "John"
          },
          "content": {
            "type": "string",
            "description": "Assignment content",
            "example": "make a database with java"
          },
          "heading": {
            "type": "array",
            "description": "The heading of the assignment",
            "example": "Do A great database",
            "items": {
              "$ref": "#/components/schemas/Rubric"
            }
          }
        },
        "required": [
          "content",
          "professor",
          "subject"
        ]
      },
      "Rating": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "points": {
            "type": "number",
            "format": "double"
          },
          "description": {
            "type": "string"
          },
          "long_description": {
            "type": "string"
          }
        }
      },
      "Rubric": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "points": {
            "type": "integer",
            "format": "int32"
          },
          "description": {
            "type": "string"
          },
          "ratings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Rating"
            }
          },
          "long_description": {
            "type": "object"
          },
          "ignore_for_scoring": {
            "type": "object"
          },
          "criterion_use_range": {
            "type": "boolean"
          }
        }
      },
      "ChatDTOResponse": {
        "type": "object",
        "properties": {
          "subject": {
            "type": "string"
          },
          "professor": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "model": {
            "type": "string"
          }
        }
      },
      "LessonReviews": {
        "type": "object",
        "properties": {
          "lessonId": {
            "type": "string"
          },
          "subject": {
            "type": "string"
          },
          "professor": {
            "type": "string"
          },
          "feedback": {
            "type": "string"
          },
          "model": {
            "type": "string"
          },
          "responseDate": {
            "type": "string"
          }
        }
      },
      "Course": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string"
          },
          "account_id": {
            "type": "integer",
            "format": "int32"
          },
          "uuid": {
            "type": "string"
          },
          "license": {
            "type": "string"
          },
          "enrollments": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Enrollment"
            }
          },
          "course_code": {
            "type": "string"
          },
          "default_view": {
            "type": "string"
          },
          "root_account_id": {
            "type": "integer",
            "format": "int32"
          },
          "enrollment_term_id": {
            "type": "integer",
            "format": "int32"
          },
          "public_syllabus_to_auth": {
            "type": "boolean"
          },
          "storage_quota_mb": {
            "type": "integer",
            "format": "int32"
          },
          "is_public_to_auth_users": {
            "type": "boolean"
          }
        }
      },
      "Enrollment": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "role_id": {
            "type": "integer",
            "format": "int32"
          },
          "user_id": {
            "type": "integer",
            "format": "int32"
          },
          "enrollment_state": {
            "type": "string"
          },
          "limit_privileges_to_course_section": {
            "type": "boolean"
          }
        }
      },
      "Assignment": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "rubric": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Rubric"
            }
          },
          "published": {
            "type": "boolean"
          },
          "due_at": {
            "type": "string",
            "format": "date-time"
          },
          "unlock_at": {
            "type": "string",
            "format": "date-time"
          },
          "lock_at": {
            "type": "string",
            "format": "date-time"
          },
          "points_possible": {
            "type": "integer",
            "format": "int32"
          },
          "grading_type": {
            "type": "string"
          },
          "assignment_group_id": {
            "type": "integer",
            "format": "int32"
          },
          "course_id": {
            "type": "integer",
            "format": "int32"
          },
          "only_visible_to_overrides": {
            "type": "boolean"
          },
          "visible_to_everyone": {
            "type": "boolean"
          },
          "locked_for_user": {
            "type": "boolean"
          },
          "submissions_download_url": {
            "type": "string"
          }
        }
      },
      "Link": {
        "type": "object",
        "properties": {
          "href": {
            "type": "string"
          },
          "templated": {
            "type": "boolean"
          }
        }
      }
    }
  }
}