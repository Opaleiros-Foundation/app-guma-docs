{
  "openapi": "3.1.0",
  "info": {
    "title": "GUMA API",
    "description": "API for reviewing assignments",
    "termsOfService": "https://github.com/Opaleiros-Foundation",
    "contact": {
      "name": "Matheus Victor",
      "url": "https://github.com/Opaleiros-Foundation",
      "email": "matheusvictorhenrique@gmail.com"
    },
    "license": {
      "name": "License of API",
